{
    "responsive-layout.desktop#minicart__related-products": {
        "title": "minicart related products",
        "children": [
            "flex-layout.row#related-products--minicart-desktop"
        ]
    },
    "flex-layout.row#related-products--minicart-desktop": {
        "title": "related products container",
        "children": [
            "flex-layout.col#related-products"
        ],
        "props": {
            "preserveLayoutOnMobile": true,
            "blockClass": [
                "related-products",
                "related-products-desktop"
            ]
        }
    },
    "responsive-layout.desktop#minicart__row": {
        "title": "product list content on desktop",
        "children": [
            "flex-layout.row#container__buttons--desktop",
            "flex-layout.row#container__top--desktop",
            "flex-layout.row#minicart__summary--desktop"
        ]
    },
    "flex-layout.row#container__buttons--desktop": {
        "title": "container buttons",
        "children": [
            "flex-layout.col#show-products__button--desktop",
            "flex-layout.col#checkout__button--desktop"
        ],
        "props": {
            "colSizing": "auto",
            "colJustify": "between",
            "marginBottom": 7
        }
    },
    "flex-layout.col#checkout__button--desktop": {
        "title": "container checkout button",
        "children": [
            "minicart-checkout-button"
        ],
        "props": {
            "width": "20%",
            "verticalAlign": "middle"
        }
    },
    "flex-layout.col#show-products__button--desktop": {
        "title": "container show products button",
        "children": [
            "link#show__products"
        ],
        "props": {
            "width": "20%",
            "verticalAlign": "middle"
        }
    },
    "product-list-content-desktop": {
        "title": "product list content",
        "children": [
            "flex-layout.row#container__bottom--desktop"
        ]
    },
    "flex-layout.row#container__top--desktop": {
        "title": "container top",
        "children": [
            "flex-layout.col#product__text",
            "rich-text#delivery-methods",
            "flex-layout.col#amount__text",
            "flex-layout.col#unit-price__text",
            "flex-layout.col#total__text"
        ],
        "props": {
            "blockClass": "container__top--desktop",
            "colJustify": "left"
        }
    },
    "flex-layout.col#product__text": {
        "title": "product column",
        "children": [
            "rich-text#product"
        ],
        "props": {
            "width": "30%"
        }
    },
    "flex-layout.col#amount__text": {
        "title": "product column",
        "children": [
            "rich-text#amount"
        ],
        "props": {
            "width": "15%"
