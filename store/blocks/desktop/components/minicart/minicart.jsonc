{
    "minicart.v2": {
        "title": "carrito de compras",
        "props": {
            "variation": "popup",
            // "linkVariationUrl": "/",
            "quantityDisplay": "always",
            "itemCountMode": "total",
            "blockClass": "minicart__v2"
        },
        "children": [
            "minicart-base-content"
        ]
    },
    "minicart-base-content": {
        "title": "minicart content",
        "blocks": [
            "minicart-empty-state"
        ],
        "children": [
            "minicart-product-list",
            "flex-layout.row#minicart__footer"
        ]
    },
    "minicart-empty-state": {
        "title": "minicart empty state",
        "children": [
            "flex-layout.row#minicart__empty"
        ]
    },
    "flex-layout.row#minicart__empty": {
        "title": "minicart empty row",
        "children": [
            "flex-layout.col#minicart__empty"
        ]
    },
    "flex-layout.col#minicart__empty": {
        "title": "minicart empty col",
        "children": [
            "icon-cart#minicart__empty--icon",
            "rich-text#minicart__empty--text"
        ]
    },
    "icon-cart#minicart__empty--icon": {
        "title": "minicart icon empty",
        "props": {
            "size": 100
        }
    },
    "flex-layout.row#minicart__footer": {
        "title": "minicart footer",
        "children": [
            "minicart-summary",
            "minicart-checkout-button"
        ]
    },
    "rich-text#minicart__empty--text": {
        "title": "empty minicart text",
        "props": {
            "text": "El carrito esta vac√≠o",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
        }
    }
}