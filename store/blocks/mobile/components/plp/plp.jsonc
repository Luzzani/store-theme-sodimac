{
    "flex-layout.row#breadcrumb--mobile": {
        "title": "breadcrumb search",
        "children": [
            "breadcrumb.search#mobile"
        ],
        "props": {
            "blockClass": "row__breadcrumb"
        }
    },
    "breadcrumb.search#mobile": {
        "title": "breadcrumb",
        "props": {
            "homeIconSize": 16,
            "showOnMobile": true
        }
    },
    "flex-layout.row#image__filter--container": {
        "title": "filter images container",
        "children": [
            "flex-layout.col#filtercontainer"
        ],
        "props": {
            "blockClass": "filter__images-container"
        }
    },
    "flex-layout.col#filtercontainer": {
        "title": "filter images container col",
        "children": [
            "flex-layout.row#images__filter--mobile"
        ],
        "props": {
            "blockClass": "filter__images-container--col"
        }
    },
    "flex-layout.row#images__filter--mobile": {
        "title": "filtro fila de imagenes",
        "children": [
            "flex-layout.col#images__filter1",
            "flex-layout.col#images__filter2",
            "flex-layout.col#images__filter3",
            "flex-layout.col#images__filter4",
            "flex-layout.col#images__filter5",
            "flex-layout.col#images__filter6",
            "flex-layout.col#images__filter7"
        ],
        "props": {
            "blockClass": [
                "image__filter--conainer",
                "image__filter--conainer--tablet"
            ]
        }
    },
    "responsive-layout.tablet#products__tablet": {
        "title": "seach content on tablet",
        "children": [
            "flex-layout.row#products--tablet"
        ]
    },
    "responsive-layout.phone#products__phone": {
        "title": "seach content on phone",
        "children": [
            "flex-layout.row#products--phone"
        ]
    },
    "flex-layout.row#products--phone": {
        "title": "products container",
        "children": [
            "search-content#content__products--mobile"
        ],
        "props": {
            "blockClass": ["product__phone--container", "product__tablet--container"]
        }
    },
    "flex-layout.row#products--tablet": {
        "title": "products container",
        "children": [
            "search-content#content__products--mobile"
        ],
        "props": {
            "blockClass": "product__tablet--container"
        }
    },
    "search-content#content__products--mobile": {
        "title": "products content",
        "blocks": [
            "gallery#mobile",
            "not-found"
        ],
        "props": {
            "blockClass": "product__tablet--childrenContainer"
        }
    },
    "gallery#mobile": {
        "title": "gallery mobile",
        "props": {
            "layouts": [
                {
                    "name": "grid",
                    "component": "GridSummary",
                    "itemsPerRow": {
                        "desktop": 4,
                        "tablet": 2,
                        "phone": 2
                    }
                },
                {
                    "name": "list",
                    "component": "ListSummary",
                    "itemsPerRow": 1
                }
            ],
            "ListSummary": "product-summary.shelf#search--list--mobile",
            "GridSummary": "product-summary.shelf#search__grid--mobile"
        }
    }
}